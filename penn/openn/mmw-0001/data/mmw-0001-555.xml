              <author>
                <persname type="authority">Mans&#803;u&#772;r ibn Muh&#803;ammad ibn Ah&#803;mad, active 14th century</persname>
                <persname type="vernacular">&#1605;&#1606;&#1589;&#1608;&#1585; &#1576;&#1606; &#1605;&#1581;&#1605;&#1583; &#1576;&#1606; &#1575;&#1581;&#1605;&#1583;&#1548;</persname>
              </author>
              <respstmt>
